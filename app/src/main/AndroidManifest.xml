            android:exported="false" />
        <activity
            android:name=".Activites.AddSubjectQuestion"
            android:exported="false" />
        <activity
            android:name=".EditingTools.EditQuestion"
            android:exported="false" />
        <activity
            android:name=".Activites.AddQuestion"
            android:exported="false" />
        <activity
            android:name=".Activites.addQuestionPaper"
            android:exported="false" />
        <activity
            android:name=".Activites.AddStudyCategories"
            android:exported="false" />
        <activity
            android:name=".Activites.AddCategoryScreen"
            android:exported="false" />
        <activity
            android:name=".Activites.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>